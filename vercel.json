{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "https://face-0.onrender.com/:path*"
      }
    ]
  }
  